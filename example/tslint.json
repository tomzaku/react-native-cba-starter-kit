{
	"defaultSeverity": "error",
	"extends": [
		"tslint-config-airbnb"
	],
	"jsRules": {},
	"rules": {
		"indent": [
			true,
			"tabs", 4
		],
		"ter-indent": [
			false
		],
		"semicolon": [
			true,
			"never"
		],
		"object-literal-sort-keys": false,
		"quotemark": [
			true,
			"single",
			"jsx-double"
		],
		"ordered-imports": true,
		"interface-name": [
			false
		],
		"max-line-length": [
			true,
			140
		],
		"no-console": [
			false
		],
		"no-empty-interface": false,
		"no-var-requires": false,
		"import-name": false,
		"no-consecutive-blank-lines": [
			true,
			0
		],
		"no-increment-decrement": false,
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-pascal-case"
		],
		"no-unused-variable": false
	},
	"rulesDirectory": []
}